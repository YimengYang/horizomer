# this file is based on the examples provided on scikit-learn's .coveragerc

[run]
omit =
    */test*
    */__init__.py
source = benchmark,distance-method,phylogeny
branch = True
incldue = */*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
omit =
    */test*
    */__init__.py
